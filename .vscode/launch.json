            "program": "${workspaceFolder}/scripts/play.py",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.vscode/.python.env"
        },

        {
            "name": "Python: [Modular_Full] Train Environment",
            "type": "debugpy",
            "request": "launch",
            // "args" : ["--task", "humanoid_full_modular", "--num_envs", "4096", "--headless", "--offscreen_render", "--video"],
            // "args" : ["--task", "humanoid_full_modular", "--num_envs", "4", "--disable_logging", "--device", "cpu", "--resume=True"],
            "args" : ["--task", "humanoid_full_modular", "--num_envs", "4", "--disable_logging", "--device", "cpu", "--headless"],
            // "args" : ["--task", "humanoid_full_modular", "--num_envs", "4", "--disable_logging", "--device", "cpu"],
