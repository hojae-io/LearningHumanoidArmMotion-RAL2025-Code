            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.vscode/.python.env"
        },
        {
            "name": "Python: [Modular_Full] Play Environment",
            "type": "debugpy",
            "request": "launch",
            // "args" : ["--task", "humanoid_full_modular_play", "--num_envs", "4", "--cpu"],
            "args" : ["--task", "humanoid_full_modular_play", "--num_envs", "3", "--device", "cpu"],
            // "args" : ["--task", "humanoid_full_modular_play", "--num_envs", "3", "--device", "cpu", "--headless"],
            // "args" : ["--task", "humanoid_full_modular_play", "--num_envs", "4096", "--headless"],
            "program": "${workspaceFolder}/scripts/play_modular.py",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.vscode/.python.env"
        },

        {
            "name": "Python: Sweep",
            "type": "debugpy",
            "request": "launch",
            "justMyCode": true,
            "args" : ["--task", "humanoid_vanilla", "--num_envs", "4096", "--headless"],
            // "args" : ["--task", "humanoid_vanilla", "--num_envs", "4096", "--headless", "--disable_logging", "--max_iterations=10"],
            "program": "${workspaceFolder}/optuna_studies/sweep.py",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.vscode/.python.env"
        },

        {
            "name": "Python: [Experiment] Play Experiment",
            "type": "debugpy",
            "request": "launch",
            // "args" : ["--num_envs", "9", "--device", "cpu"],
            // "args" : ["--num_envs", "3600", "--device", "cpu", "--headless"],
            "args" : ["--num_envs", "3600", "--headless"],
            "program": "${workspaceFolder}/scripts/experiment.py",
            "console": "integratedTerminal",
